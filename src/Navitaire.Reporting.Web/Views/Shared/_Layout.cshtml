<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Themes/SkySales/Stylesheet.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Themes/Default/jquery/jquery.ui.all.css")" rel="stylesheet" type="text/css" />    
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>  
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.custom.min.js")" type="text/javascript"></script> 
    <script src="@Url.Content("~/Scripts/tree.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Scripts/Catalog.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Scripts/settings.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Scripts/jquery.cookie.js")" type="text/javascript" ></script>
</head>

<body>
    <div id="header">
        @Html.Partial("_BannerPartial")
    </div>
    <div id="menu">
        @Html.Partial("_MenuPartial")
    </div>
    <div id="main">
        @RenderBody()
    </div>
    <div id="footer">
        @Html.Partial("_FooterPartial")
    </div>
</body>
</html>
